<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="/style1.css">
        <link rel="stylesheet" href="/css/bootstrap1.min.css">
        <title>Famas-Enregistrement</title>
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-light bg-dark py-3">
            <div class="container justify-content-between">
                <!-- Left elements -->
                <div class="d-flex">
                    <!-- Brand -->
                    <a class="navbar-brand me-2 mb-1 d-flex align-items-center" href="#">
                        <span class="text-center" style="color: aliceblue;">
                            FAMAS  
                        </span>
                    </a>
                </div>

                <ul class="navbar-nav flex-row">
                    <li class="nav-item dropdown me-3 me-lg-1">
                        <a class="nav-link " href="/acceuil" id="navbarDropdownMenuLink"
                            role="button" data-mdb-toggle="dropdown" aria-expanded="false">
                           
                            <i class="bi bi-cart"></i>
                            <span class="badge rounded-pill badge-notification bg-danger" style="color: aliceblue";>Accueil</span>
                        </a>
                    </li>
                </ul>
                
            </div>
        </nav>
        <div class="container mt-3">
            <div class="row justify-content-center">
                <div class="col-md-auto lg-6 sm-auto xs-auto">
                    <h5 class="card-title">LISTES DES MUSICIENS</h5>
                            <table class="table">
                                    {{#if musicien}}
                                        <thead class="thead-dark">
                                            <tr>
                                                
                                                <th scope="col">Nom</th>
                                                <th scope="col">Postnom</th>
                                                <th scope="col">Prenom</th>
                                                <th scope="col">Pseudo</th>
                                                <th scope="col">Fonction</th>
                                                <th scope="col">Adresse</th>
                                                <th scope="col">Telephone</th>
                                                <th scope="col">Email</th>
                                                <th scope="col">Nationalit√©</th>
                                                <th scope="col">Lieu Naissance</th>
                                                <th scope="col">Date Naissance</th>
                                                <th scope="col">Instrument</th>
                                                <th scope="col">Paroisse</th>
                                                <th scope="col">Provinceth>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each musicien }}
                                                <tr>
                                                    <th scope="row">
                                                        {{this.nom}}
                                                    </th>
                                                    
                                                    <td>{{this.postnom}}</td>
                                                    <td>{{this.prenom}}</td>
                                                    <td>{{this.pseudo}}</td>
                                                    <td>{{this.fonction}}</td>
                                                    <td>{{this.adresse}}</td>
                                                    <td>{{this.telephone}}</td>
                                                    <td>{{this.email}}</td>
                                                    <td>{{this.nationalite}}</td>
                                                    <td>{{this.lieudenaissance}}</td>
                                                    <td>{{this.datenaissance}}</td>
                                                    <td>{{this.nominst}}</td>
                                                    <td>{{this.paroisse}}</td>
                                                    <td>{{this.province}}</td>  
                                                </tr>
                                            
                                            {{/each}}
                                        </tbody>
                                    {{else}}
                                    <ul class="list-group">
                                        <li class="list-group-item">erreur</li>
                                    </ul>
                                    {{/if}}
                                </table> 
                </div>
            </div>
        </div>
    </body>
</html>




